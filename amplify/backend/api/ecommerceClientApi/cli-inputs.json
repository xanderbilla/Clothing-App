{
  "version": 1,
  "paths": {
    "/products": {
      "name": "/products",
      "lambdaFunction": "ecommerceClientProdFunc",
      "permissions": {
        "setting": "open"
      }
    },
    "/orders": {
      "name": "/orders",
      "lambdaFunction": "ecommerceClientOrdersFunc",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    }
  }
}